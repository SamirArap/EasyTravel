<div class="container">
	<div class="jumbotron">	
		<div class="row">
			<div class="col-md-3">Logo	</div>
			<div class="col-md-4">Name of agency </div>
			<div class="col-md-1">Visible </div>
			<div class="col-md-2">Number of pages </div>
		</div>

		<% @subjects.each do |subject| %>
			<div class="row">
				<div class="col-md-3"><%= image_tag(subject.logo, :class => "img-circle", size: "180x120") %>
				</div>
				<div class="col-md-4"><%= link_to subject.name, {:action => 'show', :id => subject.id } %>
				</div>
				<div class="col-md-1"><%= subject.visible%> </div>
				<div class="col-md-2">Number </div>
				<div class="col-md-2">
					<%= link_to "New Subject", {:action => 'new'}, :class => "btn btn-default btn-block" %>
					<%= link_to "New Pages", :class => "btn btn-default btn-block" %>
					<%= link_to "Back", {:controller => 'public', :action => 'index'}, :class => "btn btn-default btn-block" %>
				</div>
			</div>
		<%end%>	
	</div>
</div>
